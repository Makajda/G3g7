@inherits NoteView

<td class="title" style="@GetMonochrome()">@title</td>
@if (!Options.IsHideValue) {
    <td class="title" style="@GetMonochrome()">@Octave.Re</td>
}

@if (World.IsNext && Octave.NextTwo.IsVisible) {
    <SoView World="World.Next" Octave="Octave.NextTwo" VisibleChanged="_=> VisibleChanged.InvokeAsync()" />
}

@code {
    private string title = "Re";
}
