@inherits NoteView

<td class="title" style=@(Options.IsMonochrome ? LineCreated + GetMonochrome() : null)>@title</td>
@if (!Options.IsHideValue) {
    <td class="title" style=@GetMonochrome()>@Octave.Fa</td>
}

@if (World.IsNext) {
    <FaView World="World.Next" Octave="Octave.NextOne" LineCreated="" />
}

@code {
    [Parameter] public string LineCreated { get; set; } = "background-color: yellow;";
    private string title = "Fa";
}
